# Check the current config was properly parsed.
CONFIGS {}
REMOTE_WAIT 1
(u'CONFIGS',
 {u'Test\\.Test': {u'test.test': u'test'},
  u'Test\\=': {u'opt': u'val'},
  u'Test\\=tseT': {u'Bad\\': u'=Blah'},
  u'\\=Test\\=': {u'opt.test': u'Blah'},
  'defaults': {'global_transform': 'None', 'keep': 0, 'rate': 10}})

