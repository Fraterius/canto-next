WATCHTAGS [ "maintag\\:Static"]

# Add feed
SETCONFIGS { "Feed Static" : { "url" : "file:///tmp/canto.xml" } }
UPDATE {}
REMOTE_IGNORE 5

# Get ITEMS, should be all 5
ITEMS [ "maintag\\:Static", "maintag\\:Static 2" ]
REMOTE_WAIT 1
(u'ITEMS',
 {'maintag\\:Static': [(u'file:///tmp/canto.xml',
                        u'B. This is a test feed.'),
                       (u'file:///tmp/canto.xml',
                        u'C. This is a test feed.'),
                       (u'file:///tmp/canto.xml',
                        u'E. This is a test feed.'),
                       (u'file:///tmp/canto.xml',
                        u'D. This is a test feed.'),
                       (u'file:///tmp/canto.xml',
                        u'A. This is a test feed.')],
  'maintag\\:Static 2': []})

# Switch the filter.
SETCONFIGS { "defaults" : { "global_transform" : "sort_alphabetical" } }
REMOTE_IGNORE 6

# This should be different because of the filterset.
ITEMS [ "maintag\\:Static" ]
REMOTE_WAIT 1
(u'ITEMS',
 {'maintag\\:Static': [(u'file:///tmp/canto.xml',
                        u'A. This is a test feed.'),
                       (u'file:///tmp/canto.xml',
                        u'B. This is a test feed.'),
                       (u'file:///tmp/canto.xml',
                        u'C. This is a test feed.'),
                       (u'file:///tmp/canto.xml',
                        u'D. This is a test feed.'),
                       (u'file:///tmp/canto.xml',
                        u'E. This is a test feed.')]})
